def switch_dict(dictionary):
    switched_dict = {value: key for key, value in dictionary.items()}
    for key, value in switched_dict.items():
        print(key, ":", value)
original_dict = {
    "key1": "value1",
    "key2": "value2",
    "key3": "value3",
    "key4": "value4",
    "key5": "value5"
}

switch_dict(original_dict)
